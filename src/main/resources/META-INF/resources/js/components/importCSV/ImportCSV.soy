{namespace ImportCSV}

/**
* This renders the component's whole content.
*/
{template .render}
{@param? errorMessage: string}

<div class="import-csv-component">
	<label class="btn btn-primary" for="uploadFile">
		Please choose a file in CSV format
	</label>
	<input
			data-onchange="_handleUploadFile"
			id="uploadFile"
			type="file" />

	{$errorMessage}

	<br>
</div>
{/template}
